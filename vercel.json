{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...nextauth]"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://succinct-feedback-dapp.vercel.app",
    "DISCORD_REDIRECT_URI": "https://succinct-feedback-dapp.vercel.app/api/auth/callback/discord"
  }
}
